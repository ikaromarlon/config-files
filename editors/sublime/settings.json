{
    "auto_find_in_selection": true,
    "find_selected_text": true,
    "folder_exclude_patterns":
    [
        ".svn",
        ".git",
        ".hg",
        "CVS",
        ".vscode"
    ],
    "highlight_line": true,
    "ignored_packages":
    [
        "Vintage"
    ],
    "show_encoding": true,
    "show_line_endings": true,
    "tab_size": 4,
    "theme": "Adaptive.sublime-theme",
    "word_wrap": false,
    "shift_tab_unindent": true
}